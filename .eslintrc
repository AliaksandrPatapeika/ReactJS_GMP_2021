{
	"env": {
		"browser": true,
		"jest": true
	},
	"plugins": ["simple-import-sort"],
	"rules": {
		"simple-import-sort/imports": "error",
		"comma-dangle": ["error", "never"],
		"object-curly-spacing": ["error", "never"],
		"newline-after-var": ["error", "always"]
	},
	"parser": "@babel/eslint-parser",
	"extends": ["airbnb", "airbnb/hooks"]
}